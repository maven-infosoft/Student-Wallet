@model List<StudentERP.Models.FeeDetail>

@{
    ViewBag.Title = "List";
}

@{
    var grid = new WebGrid(Model);
    WebGridColumn[] sdsdeatil ={
                                  grid.Column(header:"Acadamic Year",format:a=>a.ay),
                                  grid.Column(header:"Shift",format:a=>a.shft),
                                  grid.Column(header:"Standard",format:a=>a.std),
                                  grid.Column(header:"Fee Heads",format:a=>a.feehd),
                                  grid.Column(header:"Amount",format:a=>a.feeamt),

                                  grid.Column(header:"Edit",format:(a)=>Html.ActionLink("Edit","Add_state",new{id=a.yssfID},"Home"))

                              };
}
<div class="dialog">

    @grid.GetHtml(tableStyle: "PGrid", headerStyle: "Header", columns: sdsdeatil)
</div>


