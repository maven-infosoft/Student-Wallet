@model StudentERP.Models.FeeHeads

@{
    ViewBag.Title = "Add";
}

@using (Html.BeginForm("Add_feeHeads", "Home", FormMethod.Post))
{
    <div class="dialog">
        <div class="panel-default">
            <table width="100%">
                <tr>
                    <th colspan="3" class="header">Add Fee Heads</th>
                </tr>

                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>

                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
                <tr>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>

                    <td class="subheader">@Html.LabelFor(lbl => lbl.fhName)</td>

                    <td class="subheader">@Html.TextBoxFor(txt => txt.fhName)</td>

                    <td>@Html.ValidationMessageFor(m => m.fhName)</td>
                </tr>
                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>

                <tr>
                    <td colspan="3" class="incenter"><input type="submit" value="Add" class="bn" /></td>
                </tr>

                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
            </table>
        </div>
    </div>

}
