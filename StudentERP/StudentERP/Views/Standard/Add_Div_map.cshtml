@model StudentERP.Models.StandardDivisionMapping

@{
    ViewBag.Title = "Add_Div_map";
}

@using (Html.BeginForm("Add_standard_wies_division", "Home", FormMethod.Post))
{
<div class="dialog">
    <div class="panel-default">
            <table width="100%">
                <tr>
                    <th colspan="3" class="header">Add Standard Wies Division</th>
                </tr>

                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
                <tr>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>

                    <td class="subheader">@Html.LabelFor(lbl => lbl.std)</td>

                    <td class="subheader">@Html.DropDownList("Standard", (IEnumerable<SelectListItem>)ViewBag.standard, "---Select---")</td>

                    <td>@Html.ValidationMessageFor(m => m.std)</td>
                </tr>
                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
                <tr>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>

                    <td class="subheader">@Html.LabelFor(lbl => lbl.divi)</td>

                    <td class="subheader">@Html.DropDownList("Division", (IEnumerable<SelectListItem>)ViewBag.div, "---Select---")</td>

                    <td>@Html.ValidationMessageFor(m => m.divi)</td>
                </tr>
                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
                <tr>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>

                    <td class="subheader">@Html.LabelFor(lbl => lbl.Year)</td>

                    <td class="subheader">@Html.DropDownList("Year", (IEnumerable<SelectListItem>)ViewBag.yr, "---Select---")</td>

                    <td>@Html.ValidationMessageFor(m => m.Year)</td>
                </tr>
                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>

                <tr>
                    <td colspan="3" class="incenter"><input type="submit" value="Add" class="bn" /></td>
                </tr>

                <tr>
                    <td colspan="3">&nbsp;</td>
                </tr>
            </table>
    </div>
</div>
        }